{
    "Application":
    {
        "Name": "Demo.RabbitConsumer",
        "Version": "0.0.0.0"
    },
    "Rabbit":
    {
        "Scale": 10,
        "ClientDeclarations": 
        {
            "Connections":
            [
                {
                    "Name":"MetricsConnection",
                    "ClientName": "MetricsLogger",
                    "ServerName": "10.0.0.215",
                    "UserName": "test",
                    "Password": "test"
                }
            ],
            "AsyncRecievers":
            [
                {
                    "Name": "TestMessage",
                    "Connection": "MetricsConnection",
                    "Queue": "metrics_queue"
                },
                {
                    "Name": "IncomingMessages",
                    "Connection": "MetricsConnection",
                    "Queue": "metrics_queue"
                }
            ]
        },
        "ServerDeclarations":
        {
            "Exchanges":
            [
                {
                    "Name": "InterTopic",
                    "Type": "Topic",
                    "AutoDelete": "False",
                    "Durable": "True",
                    "Connection": "MetricsConnection"
                }
            ],
            "Bindings":
            [
                {
                    "Queue": "metrics_queue",
                    "Connection": "MetricsConnection",
                    "Exchange": "InterTopic",
                    "SubscriptionKey": "metric"
                },
                {
                    "Queue": "metrics_queue_2",
                    "Connection": "MetricsConnection",
                    "Exchange": "InterTopic",
                    "SubscriptionKey": "metric"
                }
            ],
            "Queues":
            [
                {
                    "Name": "metrics_queue_2",
                    "Connection": "MetricsConnection",
                    "AutoDelete": "False",
                    "Durable": "True",
                    "Exclusive": "False"
                },
                {
                    "Name": "metrics_queue",
                    "Connection": "MetricsConnection",
                    "AutoDelete": "False",
                    "Durable": "True",
                    "Exclusive": "False"
                }
            ]
        } 
    }
}